.topBar{
  width: 750rpx;
height: 392rpx;
border-radius: 0 0 70rpx 70rpx;
background: linear-gradient(#787cf9 0%, #779dff 100%);
position: relative;
}
.index-container{
  background-color: rgb(239 234 234);
  height: 100vh;
}
.swiper-comtanier{
  width: 638rpx;
  height: 264rpx;
  position: absolute;
  bottom: -30%;
  left: 60rpx;
  border-radius: 40rpx;

}
swiper-item{
  border-radius: 40rpx;
background: #fff;
box-shadow: 0px 3px 3px rgba(0, 0, 0, 0.16);
}
.swiper-comtanier image{
  width: 100%;
  height: 100%;
  
}
.get-card{
  margin: 200rpx auto 0 auto;
  width: 652.08rpx;
height: 134.8rpx;
border-radius: 40rpx;
background: #779dff;
box-shadow: 0px 0px 10px rgba(64, 128, 255, 0.2);
}
.card-left-top{
  font-weight: normal;
font-size: 50rpx;
font-family: 'webfont';
color: #fff;
}
.get-card{
  display: flex;
  align-items: center;
}
.get-card-left{
  margin-left: 35rpx;
}
.card-left-bottom{
  font-weight: normal;
font-size: 22rpx;
line-height: 34rpx;
text-align: left;
color: #fff;
}
.top-get{
  width: 163rpx;
  height: 90rpx;
  border-radius: 14rpx;
  background: #fff;
  font-weight: normal;
  font-size: 33rpx;
  line-height: 90rpx;
  text-align: center;
  color: #457af9;
  margin-right: 40rpx;
  margin-left: auto;
}
.group{
  font-family: 'webfont1';
}
.second,.third{
  margin-top: 70rpx !important;
}
.third{
  font-family: 'webfont2';
}
.home-icon,.profile-icon{
  width: 58rpx;
  height: 58rpx;
 
}
.tab-bar{
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  border-radius: 40rpx 40rpx 0 0;
  left: 0;
  padding-top: 40rpx;
  background-color: white;
  z-index: 1;
}
.tab-bar>view{
  flex-grow: 1;
  height: 100%;
  align-self: center;
  display: flex;
  padding-bottom: 45rpx;
}
.home-icon image ,.profile-icon image{
  width: 58rpx ;
  height: 58rpx;
  margin: 0 auto;
}
.add-icon>view{

  margin: 0 auto;
  font-size: 56rpx;
  width: 130rpx;
  height: 130rpx;
  border-radius: 75rpx;
  position: absolute;
  top: -40rpx;
  left: 40%;
}
.add-icon{
  width: 130rpx;
  height: 130rpx;
  line-height: 130rpx;
 

}
.add{
  text-align: center;
  font-size: 80rpx;
  font-weight:bold ;
  color: white;
  border-radius:65rpx ;
  background: linear-gradient(#7057e7 0%, #a979ed 100%);
}
.btn-hover {
  transform: scale(1.1);
}
