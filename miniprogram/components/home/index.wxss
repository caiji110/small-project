.topBar{
  width: 750rpx;
height: 392rpx;
border-radius: 0 0 70rpx 70rpx;
background: linear-gradient(#787cf9 0%, #779dff 100%);
position: relative;
}
.index-container{
  background-color: rgb(239 234 234);
  height: 100vh;
}
.swiper-comtanier{
  width: 638rpx;
  height: 264rpx;
  position: absolute;
  bottom: -30%;
  left: 60rpx;
  border-radius: 40rpx;

}
swiper-item{
  border-radius: 40rpx;
background: #fff;
box-shadow: 0px 3px 3px rgba(0, 0, 0, 0.16);
}
.swiper-comtanier image{
  width: 100%;
  height: 100%;
  
}
.get-card{
  margin: 200rpx auto 0 auto;
  width: 652.08rpx;
height: 134.8rpx;
border-radius: 40rpx;
background: #779dff;
box-shadow: 0px 0px 10px rgba(64, 128, 255, 0.2);
}
.card-left-top{
  font-weight: normal;
font-size: 50rpx;
font-family: 'webfont';
color: #fff;
}
.get-card{
  display: flex;
  align-items: center;
}
.get-card-left{
  margin-left: 35rpx;
}
.card-left-bottom{
  font-weight: normal;
font-size: 22rpx;
line-height: 34rpx;
text-align: left;
color: #fff;
}
.top-get{
  width: 163rpx;
  height: 90rpx;
  border-radius: 14rpx;
  background: #fff;
  font-weight: normal;
  font-size: 33rpx;
  line-height: 90rpx;
  text-align: center;
  color: #457af9;
  margin-right: 40rpx;
  margin-left: auto;
}
.group{
  font-family: 'webfont1';
}
.second,.third{
  margin-top: 70rpx !important;
}
.third{
  font-family: 'webfont2';
}